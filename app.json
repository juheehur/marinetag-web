{
  "expo": {
    "name": "MarineTag",
    "slug": "marinetag",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.marinepal.marinetag",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "물고기 사진을 촬영하기 위해 카메라 접근 권한이 필요합니다.",
        "NSLocationWhenInUseUsageDescription": "촬영한 물고기의 위치 정보를 저장하기 위해 위치 정보에 접근합니다.",
        "NSPhotoLibraryUsageDescription": "촬영한 물고기 사진을 저장하기 위해 사진 라이브러리에 접근합니다.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-vision-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "078c9c65-0b55-483c-9dc8-c3fbbf8c7889"
      }
    }
  }
}
